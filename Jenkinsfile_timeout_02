
pipeline {
     stage('Stage1') {
       echo "This is from Stage1"
     }
    stage('Example') {
      options {
          timeout(time: 10, unit: 'SECONDS')   // timeout on this stage
      }
      steps {
          sh "sleep 30s"
      }
    }
    stage('Last Stage') {
       echo "This is from Last Stage"
    }
}
